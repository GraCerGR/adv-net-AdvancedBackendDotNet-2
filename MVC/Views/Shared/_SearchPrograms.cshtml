@model ProgramPagedListModel

<div style="margin-bottom: 10px; margin-top: 10px">
    <table>
        <tr>
            <th style="border:solid; border-width:thin">Name</th>
            <th style="border:solid; border-width:thin">Code</th>
            <th style="border:solid; border-width:thin">Language</th>
            <th style="border:solid; border-width:thin">EducationForm</th>
            <th style="border:solid; border-width:thin">Faculty</th>
            <th style="border:solid; border-width:thin">EducationLevel</th>
        </tr>
        @foreach (var item in Model.Programs)
        {
            <tr>
                <td style="border:solid; border-width:thin">@item.Name</td>
                <td style="border:solid; border-width:thin">@item.Code</td>
                <td style="border:solid; border-width:thin">@item.Language</td>
                <td style="border:solid; border-width:thin">@item.EducationForm</td>
                <td style="border:solid; border-width:thin">@item.Faculty.Name</td>
                <td style="border:solid; border-width:thin">@item.EducationLevel.Name</td>
            </tr>
        }
    </table>
</div>
